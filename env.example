# Variable names the app reads from the environment. Set these in your hosting
# panel or export in your shell. The app does not load .env files.
# Required on hosting:
SECRET_KEY=your-secret-key-here
DB_HOST=localhost
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=your_db_name

# Optional
FLASK_ENV=production
FLASK_DEBUG=False
